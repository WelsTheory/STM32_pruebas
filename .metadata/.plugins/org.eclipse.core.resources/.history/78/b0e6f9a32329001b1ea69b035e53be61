#include "main.h"
#include "sapi.h"

uint16_t tickCounter = 0;

TIM_HandleTypeDef htim11;
UART_HandleTypeDef huart2;

void boardInit(void){
	HAL_Init();
	SystemClock_Config();
	MX_GPIO_Init();
	MX_USART2_UART_Init();
	MX_TIM11_Init();
	HAL_TIM_Base_Start_IT(&htim11);
}

