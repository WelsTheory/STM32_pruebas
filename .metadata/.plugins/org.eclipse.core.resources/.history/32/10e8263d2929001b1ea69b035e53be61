#include "main.h"
#include "sapi.h"

uint16_t tiempo = 0;

int main(void)
{
	boardInit();
	while (1)
	{
		/*HAL_GPIO_TogglePin(LED1_GPIO_Port, LED1_Pin);
		HAL_GPIO_TogglePin(LED3_GPIO_Port, LED3_Pin);
		HAL_GPIO_TogglePin(LED2_GPIO_Port, LED2_Pin);
		HAL_GPIO_TogglePin(LED0_GPIO_Port, LED0_Pin);
		HAL_Delay(100);*/
		tiempo = tickRead();
		if(tiempo >= 1000){
			HAL_GPIO_TogglePin(LED0_GPIO_Port, LED0_Pin);
			tickWrite(0);
		}
	}

}
