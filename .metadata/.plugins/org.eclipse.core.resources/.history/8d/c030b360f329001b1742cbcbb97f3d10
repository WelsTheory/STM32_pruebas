#include "main.h"
#include "sapi.h"
#include "mef.h"

void MEFButton(void){
	estadoActual = BUTTON_UP;
}

void MEFButtonActualizar(void){
	switch(estadoActual){
		case BUTTON_UP:
			if(!HAL_GPIO_ReadPin(GPIOC, B0_Pin)){
					sec = 0;
			}
		break;
		case BUTTON_FALLING:
		break;
		case BUTTON_RISING:
		break;
		case BUTTON_DOWN:
		break;
	}
}

